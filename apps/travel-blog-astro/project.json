{
  "name": "travel-blog-astro",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/travel-blog-astro/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/travel-blog-astro"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "astro dev",
        "cwd": "apps/travel-blog-astro"
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/apps/cryptocurrencies-frontend"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/cryptocurrencies-frontend"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/cryptocurrencies-frontend/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
